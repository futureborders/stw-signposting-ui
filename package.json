    "start": "cross-env NODE_ENV=production ts-node --transpile-only src/server.ts",
    "build": "rimraf dist/ && tsc && yarn run build:assets",
    "build:assets": "gulp assets",
    "build:ci": "yarn build",
    "dev": "yarn build && cross-env NODE_ENV=development nodemon -e js,json,ts,html --watch src --delay 1 --exec 'ts-node' src/server.ts",
    "test": "yarn build && jest --watchAll --coverage --runInBand --detectOpenHandles --verbose",
    "test:ci": "yarn build && jest --forceExit --ci --coverage --runInBand --detectOpenHandles --verbose",
    "lint": "tsc --noEmit && eslint '{src,__test__}/**/*.{js,ts,tsx}' --fix",
    "watch:css": "nodemon -e scss -x \"gulp sass\""
  },
